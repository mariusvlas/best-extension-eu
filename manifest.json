{
  "manifest_version": 3,
  "name": "Skipper",
  "version": "0.1",
  "author": "First Last",
  "description": "Remove if website is acting weird",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
